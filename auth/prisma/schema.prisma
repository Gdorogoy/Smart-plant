generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  schemas = ["auth"]
}

model auth {
  id        String   @id @default(uuid()) @unique
  email     String @unique
  password  String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@map("auth")
  @@schema("auth")
}
