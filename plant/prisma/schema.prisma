generator client {
  provider = "prisma-client-js"
  // output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  schemas = ["plant"]
}

model plant{
  id String @id @unique @default(uuid())

  userId String
  title String
  image String 

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@schema("plant")
}
